(hugr 0)

(declare-func array.replicate
  (forall ?t type)
  (forall ?n nat)
  (where (copy ?t))
  (where (discard ?t))
  [?t] [(@ array.Array ?t ?n)]
  (ext))

(declare-func tuple.copy
  (forall ?t type)
  (where (copy ?t))
  (where (discard ?t))
  [(@ foo.tuple ?t)] [(@ foo.tuple ?t) (@ foo.tuple ?t)] (ext))
