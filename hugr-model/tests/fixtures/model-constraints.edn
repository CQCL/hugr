(hugr 0)

(declare-func array.replicate
  (param ?n nat)
  (param ?t type)
  (where (nonlinear ?t))
  [?t] [(collections.array.array ?n ?t)]
  (ext))

(declare-func array.copy
  (param ?n nat)
  (param ?t type)
  (where (nonlinear ?t))
  [(collections.array.array ?n ?t)] [(collections.array.array ?n ?t) (collections.array.array ?n ?t)] (ext))

(define-func util.copy
  (param ?t type)
  (where (nonlinear ?t))
  [?t] [?t ?t] (ext)
  (dfg [%0] [%0 %0]
    (signature (-> [?t] [?t ?t] (ext)))))
