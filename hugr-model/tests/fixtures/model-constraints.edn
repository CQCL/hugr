(hugr 0)

(declare-func array.replicate
              (param ?n core.nat)
              (param ?t core.type)
              (where (core.nonlinear ?t))
              (core.fn [?t] [(collections.array.array ?n ?t)]))

(declare-func
 array.copy
 (param ?n core.nat)
 (param ?t core.type)
 (where (core.nonlinear ?t))
 (core.fn
  [(collections.array.array ?n ?t)]
  [(collections.array.array ?n ?t)
   (collections.array.array ?n ?t)]))

(define-func util.copy
  (param ?t core.type)
  (where (core.nonlinear ?t))
  (core.fn [?t] [?t ?t])
  (dfg [%0] [%0 %0]
       (signature (core.fn [?t] [?t ?t]))))
