(hugr 0)

(define-func example.cfg
  (param ?a type)
  [?a] [?a] (ext)
  (dfg [%0] [%1]
    (signature (-> [?a] [?a] (ext)))
    (cfg [%0] [%1]
      (signature (-> [?a] [?a] (ext)))
      (cfg [%2] [%4]
        (signature (-> [(ctrl [?a])] [(ctrl [?a])] (ext)))
        (block [%2] [%4 %2]
          (signature (-> [(ctrl [?a])] [(ctrl [?a]) (ctrl [?a])] (ext)))
          (dfg [%5] [%6]
            (signature (-> [?a] [(adt [[?a] [?a]])] (ext)))
            (tag 0 [%5] [%6]
              (signature (-> [?a] [(adt [[?a] [?a]])] (ext))))))))))
