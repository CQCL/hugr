  // Just some examples
  pub fn @swap {
    %0, %1 =>
    %2, %3 = @quantum.cnot %0, %1 : [@quantum.qubit, @quantum.qubit] -> [@quantum.qubit, @quantum.qubit];
    %4, %5 = @quantum.cnot %3, %2;
    %6, %7 = @quantum.cnot %5, %4;
    %6, %7
  } : [@quantum.qubit, @quantum.qubit] -> [@quantum.qubit, @quantum.qubit];

  #[core.title("foo")]
