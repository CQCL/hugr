  fn @conditional_swap {
    %cond, %a, %b =>
    %result_a, %result_b = @core.cond %cond, %a, %b {
      %0, %a0, %b0 =>
      %a1, %b1 = @swap %a0, %b0;
      %a1, %b1
    } { %1, %a2, %b2 => %a2, %b2 };
    %result_a, %result_b
  };

  #[core.entrypoint]
